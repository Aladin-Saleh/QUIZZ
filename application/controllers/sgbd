1) Select NumEt,NomEt,PrenomEt,codeCours,NumModule
	from Module natural join Inscrit;

2)
select  CodeCours,Intitulé,DateCours
from Cours natural join Module natural join EstFaitPar
where NumModule="Informatique";

3)

Select *
from Enseignant
where Fonction="professeur" and NumEns NOT IN (Select EnsResp FROM module)

4)

Select NumEt
from Inscrit
group by NumEt
having count(distinc NumMobile) =3;

5)

Select CodeCours, NumModule, DateCours, DuréeEffective, DuréePrévue

from Cours natural join EstFaitPar

where DuréeEffective > DuréePrévu;

6)
(Select *
from Enseignant natural join Module
where NumModule="Informatique")
Intersect
(Select *
from Enseignant natural join Module
where NumModule="Maths")


create view Employe_Tim
as	Select numDep
	from Employe
	where numDep=3;

grant select on Employe_Tim to Tim

Revoke select on employe from Jim
